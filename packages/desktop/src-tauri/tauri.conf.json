{
  "$schema": "https://raw.githubusercontent.com/nicegram/nicegram-tauri-v2/main/crates/tauri-config-schema/schema.json",
  "productName": "AGENTVBX",
  "version": "0.1.0",
  "identifier": "com.agentvbx.desktop",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:5173",
    "beforeDevCommand": "",
    "beforeBuildCommand": ""
  },
  "app": {
    "title": "AGENTVBX",
    "windows": [
      {
        "title": "AGENTVBX",
        "width": 1200,
        "height": 800,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
